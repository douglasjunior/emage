{
  "name": "emage",
  "productName": "E-Mage",
  "version": "1.0.0",
  "description": "From developers to developers: a cross-platform tool to help with image optimization.",
  "private": true,
  "keywords": [
    "image-optimization",
    "electron",
    "developer-tools",
    "developer-experience"
  ],
  "author": {
    "name": "Douglas Nassif Roma Junior",
    "email": "nassifrroma@gmail.com",
    "url": "http://douglasjunior.me"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/douglasjunior/emage.git"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-push": "npm run lint"
    }
  },
  "scripts": {
    "build": "run-p build-main build-renderer",
    "build-dll": "cross-env NODE_ENV=development node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.renderer.dev.dll.js --colors",
    "build-main": "cross-env NODE_ENV=production node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.main.prod.js --colors",
    "build-renderer": "cross-env NODE_ENV=production node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.renderer.prod.js --colors",
    "dev": "cross-env START_HOT=1 node -r babel-register ./internals/scripts/CheckPortInUse.js && cross-env START_HOT=1 yarn start-renderer-dev",
    "electron-rebuild": "electron-rebuild --parallel --force --types prod,dev,optional --module-dir app",
    "package": "yarn build && electron-builder build --publish never",
    "package-all": "yarn build && electron-builder build -mwl",
    "package-linux": "yarn build && electron-builder build --linux",
    "package-win": "yarn build && electron-builder build --win --x64",
    "postinstall": "node -r babel-register internals/scripts/CheckNativeDep.js && yarn build-dll && electron-builder install-app-deps package.json",
    "prestart": "yarn build",
    "start": "cross-env NODE_ENV=production electron ./app/",
    "start-main-dev": "cross-env HOT=1 NODE_ENV=development electron -r babel-register -r babel-polyfill ./app/main.dev.js",
    "start-renderer-dev": "cross-env NODE_ENV=development node --trace-warnings -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server --config webpack.config.renderer.dev.js",
    "lint": "eslint . --cache --color"
  },
  "build": {
    "productName": "E-Mage",
    "appId": "me.douglasjunior.emage",
    "files": [
      "dist/",
      "node_modules/",
      "app.html",
      "main.prod.js",
      "main.prod.js.map",
      "package.json"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    }
  },
  "dependencies": {
    "antd": "3.8.4",
    "axios": "0.18.0",
    "bluebird": "3.5.1",
    "electron-debug": "2.0.0",
    "prop-types": "15.6.2",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-hot-loader": "4.3.5",
    "react-router-dom": "4.3.1",
    "source-map-support": "0.5.9"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-eslint": "9.0.0",
    "babel-loader": "7.1.5",
    "babel-plugin-import": "1.8.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "chalk": "2.4.1",
    "clean-webpack-plugin": "0.1.19",
    "cross-env": "5.2.0",
    "css-loader": "1.0.0",
    "detect-port": "1.2.3",
    "electron": "2.0.8",
    "electron-builder": "20.26.0",
    "electron-devtools-installer": "2.2.4",
    "electron-rebuild": "1.8.2",
    "eslint": "5.5.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-loader": "2.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.11.1",
    "html-webpack-plugin": "3.2.0",
    "husky": "1.0.0-rc.13",
    "less": "3.8.1",
    "less-loader": "4.1.0",
    "mini-css-extract-plugin": "0.4.1",
    "moment-timezone": "0.5.21",
    "node-sass": "4.9.3",
    "npm-run-all": "4.1.3",
    "optimize-css-assets-webpack-plugin": "5.0.0",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.0",
    "uglifyjs-webpack-plugin": "1.3.0",
    "webpack": "4.17.1",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.1.7",
    "webpack-merge": "4.1.4"
  }
}
